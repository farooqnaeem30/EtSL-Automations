{
  "name": "cy-bdd-tests",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:tests": "cypress run",
    "cy:cucumber:open": "yarn cypress open --env allure=true --config integrationFolder=cypress/integration ,ignoreTestFiles=*.js,testFiles=**/*.{feature,features} --browser chrome",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:clear": "rm -r allure-results/ allure-report cypress/screenshots || true",
    "allure:history": "mv -f allure-report/history allure-results/history && rm -r allure-report || true"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "prettier": "^2.1.1"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "description": "",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^1.8.3",
    "allure-commandline": "^2.13.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-cypress": "^2.10.3",
    "cucumber-html-reporter": "^5.2.0",
    "cypress": "^5.3.0",
    "cypress-cucumber-preprocessor": "^2.5.5",
    "cypress-dark": "^1.7.10",
    "cypress-file-upload": "^2.0.0",
    "cypress-xpath": "^1.3.0"
  }
}
